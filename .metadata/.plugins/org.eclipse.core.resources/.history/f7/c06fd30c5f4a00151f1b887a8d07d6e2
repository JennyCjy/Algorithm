package edu.nju.cjy.arrays;

public class MaxSum {
	public int getMaxSum(int[] nums){
		int max=0;
		int result=0;
		for (int i = 0; i < nums.length; i++) {
			result=Math.max(result+nums[i], nums[i]);
			max=result>max?result:max;
		}
		return max;
	}
	
	public static void main(String[] args) {
		
	}
}
